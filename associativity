public class Associativity {
    public static void main(String[] args) {
//        Precedence and associativity
        int a = 8;
        int b = 6;
        int x = a*b- b/a;
//        8*6=48  6/8=0.75 48-0.75=47.25
        System.out.println(x);

        int p = 6;
        int q = 1;
        int k = p*q/2;
//        6*1=6 6/2=3
        System.out.println(k);
    }
}

//OUTPUT
48
3
