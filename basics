// Addition of numbers
public class Main {
    public static void main(String[] args)
    {
        // addition of numbers
        int num1 = 9;
        int num2 = 2;
        int num3 = 5;
        int sum = num1 + num2 + num3;
        System.out.println(sum);

    }
}
//output = 16
